bootstrapPassword: "{{ settings['rancher_node']['rancher_bootstrap_password'] }}"
replicas: "{{ settings['rancher_node']['rancher_replica_count'] }}"
hostname: "{{ settings['rancher_node']['domain'] }}"
certmanager:
    version: "{{ settings['rancher_node']['cert_manager_version_to_install'] }}"
rancherImage: "{{ settings['docker_registry']['domain'] }}:5000/rancher/rancher"
rancherImageTag: "{{ settings['rancher_node']['rancher_initial_version_desired'] }}"
systemDefaultRegistry: "{{ settings['docker_registry']['domain'] }}:5000"
global:
    cattle:
        psp:
            enabled: {{ settings['rancher_node']['rancher_psp_enabled'] }}